FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value uniform (36.51084 2.553 0);
    }

    outlet
    {
        type            zeroGradient;
        value           0;
    }

    defaultFaces
    {
        type            empty;
    }
    airfoil
    {
        type            slip;
    }

    frontAndBack
    {
        type            empty;
    }
}


